{
	"compilerOptions": {
		"experimentalDecorators": true,
    "target": "es2019",
		"outDir": "out",
		"rootDir": "src",
		"module": "commonjs",
		"sourceMap": true,
		"strictPropertyInitialization": false,
		"skipLibCheck": true,
		"strictNullChecks": true
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"]
}
